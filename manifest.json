{
  "manifest_version": 2,
  "name": "FantasyGraph",
  "version": "1.0",
  "description": "Adds FanGraphs integration into the ESPN and CBSSports fantasy baseball websites.",
  "permissions": [
    "http://games.espn.go.com/flb/*", "http://*.baseball.cbssports.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "46": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://games.espn.go.com/flb/clubhouse*", "http://games.espn.go.com/flb/freeagency*", "http://*.baseball.cbssports.com/teams*", "http://*.baseball.cbssports.com/stats/stats-main*"],
      "js" : ["jquery-1.9.1.min.js", "contentscript.js"]
    }
  ]
}
